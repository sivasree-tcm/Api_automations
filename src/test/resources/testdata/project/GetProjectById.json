{
  "scenario": "Get Project By ID API Validation - Comprehensive Test Suite",
  "testCases": [
    {
      "tcId": "TC_GPD_01",
      "name": "Get project details with valid userId & projectId",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 200,
      "expectedResult": "200 OK, project details returned",
      "request": {
        "userId": 32,
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_02",
      "name": "Get project details with numeric IDs",
      "role": "SUPER_ADMIN",
      "priority": "MEDIUM",
      "status": "PASS",
      "expectedStatusCode": 200,
      "expectedResult": "200 OK / validated",
      "request": {
        "userId": 32,
        "projectId": 230
      }
    },
    {
      "tcId": "TC_GPD_03",
      "name": "Get project details with extra fields",
      "role": "SUPER_ADMIN",
      "priority": "LOW",
      "status": "PASS",
      "expectedStatusCode": 200,
      "expectedResult": "Ignored, 200 OK",
      "request": {
        "userId": 32,
        "projectId": "230",
        "extra": "x",
        "randomField": "test"
      }
    },
    {
      "tcId": "TC_GPD_04",
      "name": "Missing userId field",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request",
      "request": {
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_05",
      "name": "Missing projectId field",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request",
      "request": {
        "userId": 32
      }
    },
    {
      "tcId": "TC_GPD_06",
      "name": "userId as null",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request",
      "request": {
        "userId": null,
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_07",
      "name": "projectId as null",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
