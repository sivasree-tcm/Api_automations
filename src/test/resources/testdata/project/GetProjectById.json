      "expectedResult": "Validation error",
      "request": {
        "userId": 32,
        "projectId": ""
      }
    },
    {
      "tcId": "TC_GPD_12",
      "name": "Unauthorized request - No auth header",
      "role": "NO_AUTH",
      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 401,
      "expectedResult": "401 Unauthorized",
      "request": {
        "userId": 32,
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_13",
      "name": "Invalid token",
