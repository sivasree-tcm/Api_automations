      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 401,
      "expectedResult": "Authorization / empty response",
      "request": {
        "userId": "99999",
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_09",
      "name": "Invalid projectId - Non-existent",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
      "expectedStatusCode": 404,
      "expectedResult": "404 Not Found / validation error",
      "request": {
        "userId": 32,
        "projectId": "99999"
      }
    },
    {
      "tcId": "TC_GPD_10",
      "name": "userId as empty string",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "PASS",
