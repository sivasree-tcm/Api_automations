      "role": "SUPER_ADMIN",
      "priority": "MEDIUM",
      "status": "FAIL",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request - Validation error",
      "request": {
        "userId": "33@#$%",
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_20",
      "name": "projectId with special characters",
      "role": "SUPER_ADMIN",
      "priority": "MEDIUM",
      "status": "FAIL",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request - Validation error",
      "request": {
        "userId": 32,
        "projectId": "230@#$"
      }
    },
    {
      "tcId": "TC_GPD_21",
      "name": "Negative userId",
      "role": "SUPER_ADMIN",
      "priority": "MEDIUM",
      "status": "FAIL",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request - Invalid userId",
