      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "FAIL",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request - Input rejected",
      "request": {
        "userId": "<script>alert(1)</script>",
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_18",
      "name": "XSS injection in projectId",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "FAIL",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request - Input rejected",
      "request": {
        "userId": 32,
        "projectId": "<script>alert(1)</script>"
      }
