      "expectedResult": "400 Bad Request - Input rejected",
      "request": {
        "userId": "' OR '1'='1",
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_16",
      "name": "SQL injection in projectId",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
      "status": "FAIL",
      "expectedStatusCode": 400,
      "expectedResult": "400 Bad Request - Input rejected",
      "request": {
        "userId": 32,
        "projectId": "' OR '1'='1"
      }
    },
    {
      "tcId": "TC_GPD_17",
