      "expectedResult": "Validation error/404 error",
      "request": {
        "userId": "",
        "projectId": "230"
      }
    },
    {
      "tcId": "TC_GPD_11",
      "name": "projectId as empty string",
      "role": "SUPER_ADMIN",
      "priority": "HIGH",
